{
    "description": "Popular open source web browser. Cross-platform and available in many languages.",
    "homepage": "https://www.mozilla.org/zh-CN/firefox/new/",
    "version": "96.0.1",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/96.0.1/win64/zh-CN/Firefox%20Setup%2096.0.1.exe#/firefox.7z",
            "hash": "sha512:9ead6261aefa0a48af138dba29ba4b460e5d15a183731265e6143011797b5eb3ae1598eadd1a13cfe1a8079a5682b3ddb907b7cf1afab4eb415882ee8aa12624"
        },
        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/96.0.1/win32/zh-CN/Firefox%20Setup%2096.0.1.exe#/firefox.7z",
            "hash": "sha512:67f943af92ee34bcec32057c6ff2f5d4494cc1bf64bfc38c329dc6cccb2959a165d3c1d31b63f84a11b36bf456eecd6ae64bfd9c9354640572576916cadad4d1"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/win64/zh-CN/Firefox%20Setup%20$version.exe#/firefox.7z",
                "hash": {
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/zh-CN/$basename"
                }
            },
            "32bit": {
                "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/win32/zh-CN/Firefox%20Setup%20$version.exe#/firefox.7z",
                "hash": {
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/zh-CN/$basename"
                }
            }
        }
    }
}
