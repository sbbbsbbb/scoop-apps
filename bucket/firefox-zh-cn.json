{
    "description": "Popular open source web browser. Cross-platform and available in many languages.",
    "homepage": "https://www.mozilla.org/zh-CN/firefox/new/",
    "version": "96.0",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/96.0/win64/zh-CN/Firefox%20Setup%2096.0.exe#/firefox.7z",
            "hash": "sha512:f7ebf11e169ae6134e69ee3d68bcfd0b84a24e9e9fd53d382b7915aa292b0cc3942bfbf18156a2197a3f04fbbe08b8e6544d31a1ab726e4542685e2e5953172c"
        },
        "32bit": {
            "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/96.0/win32/zh-CN/Firefox%20Setup%2096.0.exe#/firefox.7z",
            "hash": "sha512:e1a151298b18850a5d81360699762f7360818533695f4b74a29649e5a876be72fd0ef7f32785db47ecbce8d894993ee59188c7d975e5050792e25b7062a5ef23"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/win64/zh-CN/Firefox%20Setup%20$version.exe#/firefox.7z",
                "hash": {
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/zh-CN/$basename"
                }
            },
            "32bit": {
                "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/win32/zh-CN/Firefox%20Setup%20$version.exe#/firefox.7z",
                "hash": {
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/zh-CN/$basename"
                }
            }
        }
    }
}
